{
  "name": "foodease",
  "version": "1.0.0",
  "description": "FoodEase Desktop Wrapper",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "npm run package-win",
    "package-win": "electron-packager . FoodEase --overwrite --platform=win32 --arch=x64 --icon=icons/logo.ico --prune=true --out=release-builds",
    "package-mac": "electron-packager . FoodEase --overwrite --platform=darwin --arch=x64 --icon=icons/logo.icns --prune=true --out=release-builds",
    "package-linux": "electron-packager . FoodEase --overwrite --platform=linux --arch=x64 --icon=icons/logo.png --prune=true --out=release-builds"
  },
  "repository": "https://github.com/pavanvattikala/FoodEase/",
  "keywords": [
    "foodease",
    "restaurant-management",
    "desktop-app",
    "food-ordering-system"
  ],
  "author": "Pavan Vattikala",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^13.1.0",
    "electron-packager": "^15.2.0"
  },
  "dependencies": {
    "gulp": "^4.0.2",
    "ip": "^2.0.1",
    "php-server-manager": "^1.3.3"
  }
}
